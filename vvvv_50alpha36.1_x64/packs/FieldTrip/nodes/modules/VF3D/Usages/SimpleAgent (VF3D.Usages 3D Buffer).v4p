uffer.fxh&gt;&cr;&lf;#endif&cr;&lf;&cr;&lf;&cr;&lf;////////////////////////////////////////////////////////////////////////////////////////////////&cr;&lf;// vector field function placeholder&cr;&lf;#ifndef VF3D&cr;&lf;#define VF3D placeHolderV3&cr;&lf;#endif&cr;&lf;////////////////////////////////////////////////////////////////////////////////////////////////&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;uint threadCount;&cr;&lf;StructuredBuffer&lt;float3&gt; bPos &lt;string uiname=&quot;Seed Position 3D Buffer&quot;;&gt;;&cr;&lf;RWStructuredBuffer&lt;float3&gt; Output : BACKBUFFER;&cr;&lf;&cr;&lf;float stepSizeDefault = 0.01666;&cr;&lf;StructuredBuffer&lt;float&gt; stepSizeBuffer;&cr;&lf;float maxDistDefualt &lt;string uiname=&quot;Maximum Distance from Seed Position&quot;;&gt; = 5;&cr;&lf;StructuredBuffer&lt;float&gt; maxDistBuffer;&cr;&lf;float resetAll;&cr;&lf;StructuredBuffer&lt;float&gt; resetBuffer;&cr;&lf;&cr;&lf;//GROUPSIZE&cr;&lf;[numthreads(64, 1, 1)]&cr;&lf;void CS_SimpleAgent( uint3 dtid : SV_DispatchThreadID )&cr;&lf;{&cr;&lf;&tab;if (dtid.x &gt;= threadCount) { return; }&cr;&lf;&tab;&cr;&lf;&tab;float reset = max(resetAll, resetBuffer[dtid.x % sbSize(resetBuffer)]);&cr;&lf;&cr;&lf;&tab;float maxDist = sbLoad(maxDistBuffer, maxDistDefualt, dtid.x);&cr;&lf;&tab;if (reset |||| maxDist &lt; distance(Output[dtid.x], bPos[dtid.x])) &cr;&lf;&tab;{ Output[dtid.x] = bPos[dtid.x]; }&cr;&lf;&tab;&cr;&lf;&tab;float stepSize = sbLoad(stepSizeBuffer, stepSizeDefault, dtid.x);&cr;&lf;&tab;&cr;&lf;&tab;#if (INTEGRATIONMODE==1) &cr;&lf;&tab;calcRK2V3(VF3D, Output[dtid.x], stepSize);&cr;&lf;&tab;#elif (INTEGRATIONMODE==2) &cr;&lf;&tab;calcRK4V3(VF3D, Output[dtid.x], stepSize);&cr;&lf;&tab;#else &cr;&lf;&tab;calcEulerV3(VF3D, Output[dtid.x], stepSize);&cr;&lf;&tab;#endif&cr;&lf;}&cr;&lf;&cr;&lf;&cr;&lf;technique11 SimpleAgent&cr;&lf;{&cr;&lf;&tab;pass P0&cr;&lf;&tab;{&cr;&lf;&tab;&tab;SetComputeShader( CompileShader( cs_5_0, CS_SimpleAgent() ) );&cr;&lf;&tab;}&cr;&lf;}&cr;&lf;&cr;&l