   <PIN pinname="ShaderCode" visible="1" slicecount="1" values="|// INTEGRATIONMODE: Integrate postion by vector field&cr;&lf;// 0:Euler, 1:RK2, 2:RK4&cr;&lf;&cr;&lf;#ifndef FIELDS_FXH&cr;&lf;#include &lt;packs\happy.fxh\fields.fxh&gt;&cr;&lf;#endif&cr;&lf;&cr;&lf;////////////////////////////////////////////////////////////////////////////////////////////////&cr;&lf;//&cr;&lf;//&tab;&tab;3D Vector Field Function Class&cr;&lf;//&cr;&lf;////////////////////////////////////////////////////////////////////////////////////////////////&cr;&lf;// VF3D START&cr;&lf;&cr;&lf;&tab;// placeholders for two input firld classes&cr;&lf;&tab;////////////////////////////////////////////////////////////////////////////////////////////////&cr;&lf;&tab;// INPUT1 START&cr;&lf;&tab;class cInput1 : iVectorField3D &cr;&lf;&tab;{&cr;&lf;&tab;&tab;&cr;&lf;&tab;&tab;float3 Vector (float3 p)&cr;&lf;&tab;&tab;{&cr;&lf;&tab;&tab;&tab;return float3(.01,0,0);&cr;&lf;&tab;&tab;}&cr;&lf;&tab;};&cr;&lf;&tab;cInput1 INPUT1;&cr;&lf;&tab;// INPUT2 END&cr;&lf;&tab;////////////////////////////////////////////////////////////////////////////////////////////////&cr;&lf;&tab;&cr;&lf;&tab;////////////////////////////////////////////////////////////////////////////////////////////////&cr;&lf;&tab;// INPUT2 START&cr;&lf;// paramaters&cr;&lf;float3 direction : DIRECTION = float3(1.0, 0.0, 0.0);&cr;&lf;&tab;&cr;&lf;// classes&cr;&lf;class cDirection3D : iVectorField3D &cr;&lf;{&cr;&lf;&tab;float3 Vector (float3 p)&cr;&lf;&tab;{&cr;&lf;&tab;&tab;//return direction;  // getting glitches???&cr;&lf;&tab;&tab;return mad(p, .00001, direction);&cr;&lf;&tab;}&cr;&lf;};&cr;&lf;&cr;&lf;//final field function class instance&cr;&lf;cDirection3D INPUT2; &cr;&lf;&cr;&lf;// INPUT2 END&cr;&lf;&tab;////////////////////////////////////////////////////////////////////////////////////////////////&cr;&lf;class cAdd : iVectorField3D &cr;&lf;{&cr;&lf;&tab;float3 Vector (float3 p)&cr;&lf;&tab;{&cr;&lf;&tab;&tab;return INPUT1.Vector(p) + INPUT2.Vector(p);&cr;&lf;&tab;}&cr;&lf;};&cr;&lf;&cr;&lf;//final field function class instance&cr;&lf;cAdd VF3D; &cr;&lf;&cr;&lf;// VF3D END&cr;&lf;////////////////////////////////////////////////////////////////////////////////////////////////&cr;&lf;&cr;&lf;&cr;&lf;uint threadCount;&cr;&lf;StructuredBuffer&lt;float3&gt; bPos &lt;string uiname=&quot;Seed Position 3D Buffer&quot;;&gt;;&cr;&lf;&cr;&lf;&cr;&lf;RWStructuredBuffer&lt;float3&gt; Output : BACKBUFFER;&cr;&lf;uint stepCount = 12;&cr;&lf;float stepSize = 0.01666;&cr;&lf;&cr;&lf;//GROUPSIZE&cr;&lf;[numthreads(64, 1, 1)]&cr;&lf;void CS_StreamLine( uint3 dtid : SV_DispatchThreadID )&cr;&lf;{&cr;&lf;&cr;&lf;&tab;if (dtid.x &gt;= threadCount) { return; }&cr;&lf;&cr;&lf;&tab;Output[dtid.x] = bPos[dtid.x];&cr;&lf;&tab;float3 p = bPos[dtid.x];&cr;&lf;&tab;for (uint i = 0; i &lt; stepCount; i++)&cr;&lf;&tab;{&cr;&lf;&tab;&tab;uint index = dtid.x * stepCount + i;&cr;&lf;&tab;&tab;p = integrate(VF3D, p, stepSize); &cr;&lf;&tab;&tab;Output[index] =  p;&cr;&lf;&tab;}&cr;&lf;}&cr;&lf;&cr;&lf;&cr;&lf;technique11 StreamLine&cr;&lf;{&cr;&lf;&tab;pass P0&cr;&lf;&tab;{&cr;&lf;&tab;&tab;SetComputeShader( CompileShader( cs_5_0, CS_StreamLine() ) );&cr;&lf;&tab;}&cr;&lf;}&cr;&lf;&cr;&lf;|">
   </PIN>
   <PIN pinname="ConfigShader" slicecount="1" values="|&cr;&lf;#ifndef CALC_FXH&cr;&lf;#include &lt;packs\happy.fxh\calc.fxh&gt;&cr;&lf;#endif&cr;&lf;&cr;&lf;#ifndef SBUFFER_FXH&cr;&lf;#include &lt;packs\happy.fxh\sbuffer.fxh&gt;&cr;&lf;#endif&cr;&lf;&cr;&lf;&cr;&lf;////////////////////////////////////////////////////////////////////////////////////////////////&cr;&lf;// Input placeholder&cr;&lf;#ifndef VF2D&cr;&lf;#define VF2D normalize&cr;&lf;#endif&cr;&lf;////////////////////////////////////////////////////////////////////////////////////////////////&cr;&lf;// integration method selector&cr;&lf;    #if (INTEGRATIONMODE==1) &cr;&lf;&tab;#define integrate calcRK2V2&cr;&lf;&tab;#elif (INTEGRATIONMODE==2) &cr;&lf;&tab;#define integrate calcRK4V2&cr;&lf;&tab;#else &cr;&lf;&tab;#define integrate calcEulerV2&cr;&lf;&tab;#endif&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;uint threadCount;&cr;&lf;StructuredBuffer&lt;float2&gt; bPos &lt;string uiname=&quot;Seed Position 2D Buffer&quot;;&gt;;&cr;&lf;RWStructuredBuffer&lt;float2&gt; Output : BACKBUFFER;&cr;&lf;&cr;&lf;uint pathSize &lt;string uiname=&quot;Points Per Path&quot;;&gt; = 32;&cr;&lf;float maxDist &lt;string uiname=&quot;Maximum Distance from Seed Position&quot;;&gt; = 5;&cr;&lf;&cr;&lf;float stepSizeDefault &lt;string uiname=&quot;Step Size Defualt&quot;;&gt; = 0.01666;&cr;&lf;StructuredBuffer&lt;float&gt; stepSizeBuffer &lt;string uiname=&quot;Step Size Buffer&quot;;&gt;;&cr;&lf;&cr;&lf;float resetAll &lt;string uiname=&quot;Reset All&quot;;&gt;;&cr;&lf;StructuredBuffer&lt;float&gt; resetBuffer &lt;string uiname=&quot;Reset Buffer&quot;;&gt;;&cr;&lf;&cr;&lf;&cr;&lf;//GROUPSIZE&cr;&lf;[numthreads(64, 1, 1)]&cr;&lf;void CS_PathLine( uint3 dtid : SV_DispatchThreadID )&cr;&lf;{&cr;&lf;&cr;&lf;&tab;if (dtid.x &gt;= threadCount) { return; }&cr;&lf;&tab;uint pathIndex = dtid.x % pathSize;&cr;&lf;&tab;uint seedIndex = floor(dtid.x / pathSize);&cr;&lf;&tab;&cr;&lf;&tab;float reset = max(resetAll, resetBuffer[seedIndex % sbSize(resetBuffer)]);&cr;&lf;&tab;&cr;&lf;&tab;if (pathIndex != 0) // not leader&cr;&lf;&tab;{&cr;&lf;&tab;&tab;uint leaderIndex = dtid.x - pathIndex;&cr;&lf;&tab;&tab;if (reset |||| maxDist &lt; distance(Output[leaderIndex], bPos[seedIndex])) &cr;&lf;&tab;&tab;Output[dtid.x] = bPos[seedIndex];&cr;&lf;&tab;&tab;else &cr;&lf;&tab;&tab;Output[dtid.x] = Output[dtid.x-1];&lf;&tab;}&cr;&lf;&tab;&cr;&lf;&tab;else // leader&cr;&lf;&tab;{&cr;&lf;&tab;&tab;if (reset |||| maxDist &lt; distance(Output[dtid.x], bPos[seedIndex])) &cr;&lf;&tab;&tab;Output[dtid.x] = bPos[seedIndex];&cr;&lf;&tab;&tab;else &cr;&lf;&tab;&tab;{&cr;&lf;&tab;&tab;&tab;float stepSize = sbLoad(stepSizeBuffer, stepSizeDefault, dtid.x);&cr;&lf;&tab;&tab;&tab;integrate(VF2D, Output[dtid.x], stepSize);&lf;&tab;&tab;}&lf;&tab;}&cr;&lf;&tab;&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;}&cr;&lf;&cr;&lf;&cr;&lf;technique11 PathLine&cr;&lf;{&cr;&lf;&tab;pass P0&cr;&lf;&tab;{&cr;&lf;&tab;&tab;SetComputeShader( CompileShader( cs_5_0, CS_PathLine() ) );&cr;&lf;&tab;}&cr;&lf;}&cr;&lf;&cr;&lf;|">
   </PIN>
