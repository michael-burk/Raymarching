   <PIN pinname="ShaderCode" visible="1" slicecount="1" values="|text&cr;&lf;//@author: Everyoneishappy&cr;&lf;//@help: Render WorldPos to uv layout&cr;&lf;//@tags: UV&cr;&lf;//@credits: &cr;&lf;&cr;&lf;#ifndef SF3D&cr;&lf;#define SF3D length // Just a place holder&cr;&lf;#endif&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;cbuffer cbPerDraw : register( b0 )&cr;&lf;{&cr;&lf;&tab;float4x4 tP : PROJECTION;&cr;&lf;&tab;float4x4 tW : WORLD;&cr;&lf;&cr;&lf;};&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;struct VS_IN&cr;&lf;{&cr;&lf;&tab;float4 Pos : POSITION;&cr;&lf;&tab;float4 Norm : NORMAL;&cr;&lf;&tab;float4 UV : TEXCOORD0;&cr;&lf;&cr;&lf;};&cr;&lf;&cr;&lf;&cr;&lf;struct vs2ps&cr;&lf;{&cr;&lf;    float4 pPos: SV_POSITION;&cr;&lf;&tab;float4 Norm : NORMAL;&cr;&lf;    float4 UV: TEXCOORD0;&cr;&lf;&tab;float4 PosW: TEXCOORD01;&cr;&lf;};&cr;&lf;&cr;&lf;&cr;&lf;vs2ps VS(VS_IN input)&cr;&lf;{&cr;&lf;    vs2ps Out = (vs2ps)0;&cr;&lf;  &cr;&lf;&tab;//transform position to UV&cr;&lf;&tab;Out.pPos = mul((input.UV-.5), tP);&cr;&lf;&tab;Out.pPos.y *= -1.0f;&cr;&lf;&tab;Out.pPos.z=0;&cr;&lf;&tab;&cr;&lf;&tab;Out.Norm = normalize(mul(input.Norm,tW));&cr;&lf;&tab;&cr;&lf;    Out.UV = input.UV;&cr;&lf;&tab;Out.PosW  = mul(input.Pos,tW);&cr;&lf;&cr;&lf;&tab;&cr;&lf;    return Out;&cr;&lf;}&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;float4 PS(vs2ps In): SV_Target&cr;&lf;{&cr;&lf;    float4 col = 1;&cr;&lf;&tab;float3 p = In.PosW.xyz;&cr;&lf;&tab;&cr;&lf;&tab;col.rgb = SF3D(p);&cr;&lf;    return col;&cr;&lf;}&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;technique11 BakeTexture&cr;&lf;{&cr;&lf;&tab;pass P0&cr;&lf;&tab;{&cr;&lf;&tab;&tab;SetVertexShader( CompileShader( vs_5_0, VS() ) );&cr;&lf;&tab;&tab;SetPixelShader( CompileShader( ps_5_0, PS() ) );&cr;&lf;&tab;}&cr;&lf;}&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;|">
   </PIN>
   <PIN pinname="ConfigShader" slicecount="1" values="|&cr;&lf;//@author: Everyoneishappy&cr;&lf;//@help: Render WorldPos to uv layout&cr;&lf;//@tags: UV&cr;&lf;//@credits: &cr;&lf;&cr;&lf;&cr;&lf;#ifndef CALC_FXH&cr;&lf;#include &lt;packs\happy.fxh\calc.fxh&gt;&cr;&lf;#endif&cr;&lf;&cr;&lf;////////////////////////////////////////////////////&cr;&lf;#ifndef SF3D&cr;&lf;#ifndef NOISE_FXH&cr;&lf;#include&lt;packs\happy.fxh\noise.fxh&gt;&cr;&lf;#endif&cr;&lf;&cr;&lf;float defaultFunction(float3 p)&cr;&lf;{&cr;&lf;&tab;float noise = valueNoise(p*16)*1.8;&cr;&lf;&tab;return noise;&cr;&lf;}&cr;&lf;#define SF3D defaultFunction&cr;&lf;#endif&cr;&lf;////////////////////////////////////////////////////&cr;&lf;&cr;&lf;&cr;&lf;Texture2D texTransfer &lt;string uiname=&quot;Transfer Function Texture&quot;;&gt;;&cr;&lf;float transferStart = 0.0;&cr;&lf;float TransferEnd = 1.0;&cr;&lf;Texture2D texBackFace;&cr;&lf;&cr;&lf;SamplerState g_samLinear : IMMUTABLE&cr;&lf;{&cr;&lf;    Filter = MIN_MAG_MIP_LINEAR;&cr;&lf;    AddressU = CLAMP;&cr;&lf;    AddressV = CLAMP;&cr;&lf;&tab;AddressW = CLAMP;&cr;&lf;};&cr;&lf;&cr;&lf; &cr;&lf;cbuffer cbPerDraw : register( b0 )&cr;&lf;{&cr;&lf;&tab;float4x4 tVP : VIEWPROJECTION;&cr;&lf;&tab;float alpha &lt;float uimin=0.0; float uimax=1.0;&gt; = 1; &cr;&lf;&tab;uint maxSamples = 256;&cr;&lf;&cr;&lf;};&cr;&lf;&cr;&lf;&cr;&lf;cbuffer cbPerObj : register( b1 )&cr;&lf;{&cr;&lf;&tab;float4x4 tW : WORLD;&cr;&lf;&cr;&lf;};&cr;&lf;&cr;&lf;struct VS_IN&cr;&lf;{&cr;&lf;    float4 Pos  : POSITION;&cr;&lf;};&cr;&lf;&cr;&lf;struct vs2ps&cr;&lf;{&cr;&lf;    float4 PosWVP: SV_POSITION;&cr;&lf;&tab;float4 PosW : TEXCOORD1;&cr;&lf;};&cr;&lf;&cr;&lf;vs2ps VS(VS_IN input)&cr;&lf;{&cr;&lf;    vs2ps Out = (vs2ps)0;&cr;&lf;    Out.PosWVP  = mul(input.Pos,mul(tW,tVP));&cr;&lf;&tab;Out.PosW  = mul(input.Pos, tW);&cr;&lf;    return Out;&cr;&lf;}&cr;&lf;&cr;&lf;struct PS_OUT&cr;&lf;{&cr;&lf;&tab;float4 Color:SV_TARGET;&cr;&lf;&tab;#if WRITEDEPTH == 1&cr;&lf;&tab;float Depth:SV_DEPTH;&cr;&lf;&tab;#endif&cr;&lf;};&cr;&lf;&cr;&lf;&cr;&lf;float4x4 tVI:VIEWINVERSE;&cr;&lf;float4x4 tPI:PROJECTIONINVERSE;&cr;&lf;&tab;// normal projection&cr;&lf;&tab;&tab;&cr;&lf;&cr;&lf;float stepSize = 0.001;&cr;&lf;float4x4 Modelview : VIEW;&cr;&lf;&cr;&lf;float2 R : TARGETSIZE;&cr;&lf;&cr;&lf;bool rayRange(float2 uv, out float t0, out float t1)&cr;&lf;{&cr;&lf;&tab;texBackFace.Sample(g_samLinear, uv, 0);&lf;}&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;PS_OUT PS_TransferTexture (vs2ps In)&cr;&lf;{&cr;&lf;&tab;PS_OUT Out;&cr;&lf;&tab;&cr;&lf;&tab;float3 rayStart = In.PosW.xyz;&cr;&lf;    float3 rayStop = texBackFace.Sample(g_samLinear, In.PosWVP.xy/R, 0).xyz;&cr;&lf;    &cr;&lf;&cr;&lf;    // Perform the ray marching:&cr;&lf;    float3 pos = rayStart;&cr;&lf;    float3 step = normalize(rayStop-rayStart) * stepSize;&cr;&lf;    float travel = distance(rayStop, rayStart);&cr;&lf;&tab;float4 col = 0;&cr;&lf;&tab;#if WRITEDEPTH == 1&cr;&lf;&tab;Out.Depth=999999;&cr;&lf;&tab;#endif&cr;&lf;&tab;&cr;&lf;    for (uint i=0; i &lt; maxSamples &amp;&amp; travel &gt; 0.0; ++i, pos += step, travel -= stepSize) {&cr;&lf;    &cr;&lf;    &tab;// Sample from the scalar function and map to 0-1&cr;&lf;    &tab;float volSample = SF3D(pos);&cr;&lf;    &tab;volSample = (volSample - transferStart) / (TransferEnd - transferStart);&cr;&lf;    &tab;&cr;&lf;    &tab;float4 colSample = texTransfer.SampleLevel(g_samLinear, float2(volSample, .5), 0);&cr;&lf;    &tab;colSample.a *= alpha;&cr;&lf;    &tab;colSample.rgb *= colSample.a;&cr;&lf;        col = (1.0f - col.a) * colSample + col;     &cr;&lf;     &cr;&lf;        //break from the loop when alpha gets high enough&cr;&lf;        if(col.a &gt;= .95f)  &cr;&lf;    &tab;{&cr;&lf;    &tab;&tab;#if WRITEDEPTH == 1&cr;&lf;&tab;&tab;&tab;float4 PosWVP=mul(float4(pos.xyz,1),tVP);&cr;&lf;&tab;&tab;&tab;Out.Depth=PosWVP.z/PosWVP.w;&cr;&lf;&tab;&tab;&tab;#endif&cr;&lf;    &tab;&tab;break;&lf;    &tab;}&cr;&lf;&cr;&lf;    }&cr;&lf;&cr;&lf;&tab;Out.Color=col;&cr;&lf;&tab;&cr;&lf;&tab;&cr;&lf;&tab;return Out;&cr;&lf;}&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;&cr;&lf;technique10 ScalarVolumeTransferTexture&cr;&lf;{&cr;&lf;&tab;pass P0&cr;&lf;&tab;{&cr;&lf;&tab;&tab;SetVertexShader( CompileShader( vs_5_0, VS() ) );&cr;&lf;&tab;&tab;SetPixelShader( CompileShader( ps_5_0, PS_TransferTexture() ) );&cr;&lf;&tab;}&cr;&lf;}&cr;&lf;&cr;&lf;&cr;&lf;|">
   </PIN>
